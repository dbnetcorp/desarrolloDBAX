<?xml version="1.0"?>
<configuration>
	<system.web>
		<sessionState timeout="20"/>
		<customErrors mode="Off"/>
		<authentication mode="Forms">
			<forms loginUrl="~/dbnFw5/dbnLogin.aspx" defaultUrl="~/dbnet.dbax/Home.aspx" timeout="30"/>
		</authentication>
		<compilation debug="true" targetFramework="4.0">
			<assemblies>
				<add assembly="System.Design, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A"/>
				<add assembly="System.Web.Extensions.Design, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
				<add assembly="System.Windows.Forms, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089"/>
				<add assembly="System.Web.Abstractions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/></assemblies>
		</compilation>
		<authorization>
			<!-- Permitir solo usuarios autentificados -->
			<deny users="?"/>
			<allow roles="Usuario"/>
		</authorization>
		<pages controlRenderingCompatibilityVersion="4.0" clientIDMode="AutoID"></pages>
	</system.web>
	<location path="dbnet.dbax/librerias">
		<system.web>
			<authorization>
				<allow users="*"/>
			</authorization>
		</system.web>
	</location>
	<location path="librerias">
		<system.web>
			<authorization>
				<allow users="*"/>
			</authorization>
		</system.web>
	</location>
	<location path="dbnFw5/dbnLogin.aspx">
		<system.web>
			<authorization>
				<allow users="*"/>
			</authorization>
		</system.web>
	</location>
	<location path="prisma_como.html">
		<system.web>
			<authorization>
				<allow users="*"/>
			</authorization>
		</system.web>
	</location>
	<location path="dbnet.dbax">
		<system.web>
			<authorization>
				<deny users="?"/>
			</authorization>
		</system.web>
	</location>
	<location path="dbnFw5">
		<system.web>
			<authorization>
				<deny users="?"/>
			</authorization>
		</system.web>
	</location>
	<appSettings file="setting.config"/>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="Ionic.Zip" publicKeyToken="edbe51ad942a3f5c" culture="neutral"/>
				<bindingRedirect oldVersion="0.0.0.0-1.9.1.8" newVersion="1.9.1.8"/>
			</dependentAssembly>
		</assemblyBinding>
	</runtime>
</configuration>
